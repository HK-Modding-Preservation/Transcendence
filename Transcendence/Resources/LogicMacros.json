{
    "CHAOSORB": "CHAOS_ORB_ON + $EQUIPPED_TRANSCENDENCE_CHARM[Chaos_Orb]",
    "ANTIGRAV": "ANTIGRAVITY_AMULET_ON + ($EQUIPPED_TRANSCENDENCE_CHARM[Antigravity_Amulet] | CHAOSORB)",
    "BLUEMOTH": "($EQUIPPED_TRANSCENDENCE_CHARM[Bluemoth_Wings] | CHAOSORB) + (BLUEMOTH_WINGS_ON | (BLUEMOTH_WINGS_ON_GEO + Can_Replenish_Geo))",
    "ANTIGRAVWINGS": "BLUEMOTH | (ANTIGRAV + WINGS)",
    "SNAILSOUL": "SNAIL_SOUL_ON + ($EQUIPPED_TRANSCENDENCE_CHARM[Snail_Soul] | CHAOSORB) + FIREBALL",
    "SNAILSLASH": "SNAIL_SLASH_ON + ($EQUIPPED_TRANSCENDENCE_CHARM[Snail_Slash] | CHAOSORB)",
    "GREEDSONG": "GREEDSONG_ON + $EQUIPPED_TRANSCENDENCE_CHARM[Greedsong] + 2MASKS",
    "MILLIBELLE": "MILLIBELLES_BLESSING_ON + $EQUIPPED_TRANSCENDENCE_CHARM[Millibelle's_Blessing]",
    "NITRO": "NITRO_CRYSTAL_ON + ($EQUIPPED_TRANSCENDENCE_CHARM[Nitro_Crystal] | CHAOSORB)",
    "CRYSTALMASTER": "($EQUIPPED_TRANSCENDENCE_CHARM[Crystalmaster] | CHAOSORB) + (CRYSTALMASTER_ON | (CRYSTALMASTER_ON_GEO + Can_Replenish_Geo))",
    "YEET": "CRYSTALMASTER + ANTIGRAV",

    "SNAILSLASH_WITH_AIRSTALL": "SNAILSLASH + SPELLAIRSTALL"
}