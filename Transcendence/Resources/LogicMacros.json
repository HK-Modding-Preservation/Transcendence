{
    "MILLIBELLE": "MILLIBELLES_BLESSING_ON + $EQUIPPED_TRANSCENDENCE_CHARM[Millibelle's_Blessing]",
    "ANTIGRAV": "ANTIGRAVITY_AMULET_ON + $EQUIPPED_TRANSCENDENCE_CHARM[Antigravity_Amulet]",
    "BLUEMOTH": "$EQUIPPED_TRANSCENDENCE_CHARM[Bluemoth_Wings] + Can_Replenish_Geo + (BLUEMOTH_WINGS_ON | (BLUEMOTH_WINGS_ON_GEO + (MILLIBELLE | (Can_Visit_Lemm + Defeated_Colosseum_1))))",
    "ANTIGRAVWINGS": "BLUEMOTH | (ANTIGRAV + WINGS)",
    "SNAILSOUL": "SNAIL_SOUL_ON + $EQUIPPED_TRANSCENDENCE_CHARM[Snail_Soul]",
    "SNAILSLASH": "SNAIL_SLASH_ON + $EQUIPPED_TRANSCENDENCE_CHARM[Snail_Slash]",
    "NITRO": "NITRO_CRYSTAL_ON + $EQUIPPED_TRANSCENDENCE_CHARM[Nitro_Crystal]",
    "VESPA": "VESPAS_VENGEANCE_ON + $EQUIPPED_TRANSCENDENCE_CHARM[Vespa's_Vengeance]",
    "CRYSTALMASTER": "$EQUIPPED_TRANSCENDENCE_CHARM[Crystalmaster] + Can_Replenish_Geo + (CRYSTALMASTER_ON | (CRYSTALMASTER_ON_GEO + (MILLIBELLE | (Can_Visit_Lemm + Defeated_Colosseum_1))))",
    "YEET": "CRYSTALMASTER + ANTIGRAV",

    "SNAILSLASH_WITH_AIRSTALL": "SNAILSLASH + SPELLAIRSTALL",

    "LEFTBALDURS": "ORIG | (LEFTSUPERDASH + NITRO) | (CRYSTALMASTER + LEFTSLASH)",
    "RIGHTBALDURS": "ORIG | (RIGHTSUPERDASH + NITRO) | (CRYSTALMASTER + RIGHTSLASH)",
    "ALLBALDURS": "ORIG | (SCREAM + VESPA)",
    "BOSS": "ORIG | (SNAILSOUL + FIREBALL + NAILCOMBAT + (ANYDASH | MILDCOMBATSKIPS))",
    "BOSSFLUKE": "ORIG | (SNAILSOUL + FIREBALL)",
    "COMBAT[Flukemarm]": "ORIG | (SNAILSOUL + FIREBALL)"
}